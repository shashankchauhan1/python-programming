//1> star pattern

n=int(input("enter a number: "))
i=1
while(i<=n):
    j=1
    while(j<=i):
        print('*',end=' ')
        j=j+1
    print()
    i=i+1


//2> pattern of number comes in i

n=int(input("enter a number: "))
i=1
while(i<=n):
    j=1
    while(j<=i):
        print(i,end=' ')
        j=j+1
    print()
    i=i+1


